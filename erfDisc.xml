<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="erfDisc">
		<node name="input_params">
			<real name="erfDisc">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				erfDisc_init_result(erfDisc)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
			 	let
					r = errFunc(length(p_in)) * weight
					a = atan2(p_in.y, p_in.x)
				in
					vec2(cos(a),sin(a)) * r
			]]>
		</string>
	</transform_def>


</user_transforms>
