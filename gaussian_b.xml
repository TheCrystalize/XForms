<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="gaussian_b">
		<!-- Crystalize made this, based on work by bezo97 -->
		<node name="input_params">
			<real name="gaussian_b" >1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_gauss_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				gaussian_b_init_result(gaussian_b)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					a = gauss_rnd_0 * twopi()
					s = sqrt(abs(gauss_rnd_1)) * weight
				in
					vec2(cos(a), sin(a)) * s + p_in
			]]>
		</string>
	</transform_def>

</user_transforms>
