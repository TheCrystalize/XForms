<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="outsideEllipse">
		<node name="input_params">
			<real name="outsideEllipse">1</real>
			<real name="outsideEllipse_A">1</real>
		</node>

		<node name="internal_params">
			<real name="scale" />
			<real name="a" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				outsideEllipse_init_result(outsideEllipse, outsideEllipse_A)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					c1 = ((a + 1) / 2) * z
					c2 = (a - 1) / (z * 2)

				in
					toVec2(c1 + c2) * scale
			]]>
		</string>
	</transform_def>


</user_transforms>
