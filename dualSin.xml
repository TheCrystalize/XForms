<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="dualSin">
		<node name="input_params">
			<real name="dualSin">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				dualSin_init_result(dualSin)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(sin(dual(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
