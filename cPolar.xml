<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="cPolar">
		<node name="input_params">
			<real name="cPolar">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				cPolar_init_result(cPolar)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					c = r() * 2 / pi() * log(sqrt(z + 1) / sqrt(z - 1))
				in
					toVec2(c) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
