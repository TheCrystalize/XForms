<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="expermintal_real">
		<node name="input_params">
			<real name="expermintal_real">1</real>
			<real name="expermintal_real_p">3</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="p" />
		</node>

		<int name="num_unit_randoms">1</int>

		<string name="winter_init_function">
			<![CDATA[
				expermintal_real_init_result(expermintal_real, expermintal_real_p)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in.x, p_in.y)
					c = complex(p, 0.0)
					m = (z + c) / (-c * z + 1.0)
				in
					toVec2(m) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
