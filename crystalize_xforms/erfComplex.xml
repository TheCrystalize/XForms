<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="erfComplex">
		<node name="input_params">
			<real name="erfComplex">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				erfComplex_init_result(erfComplex)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					xpy = p_in.x + p_in.y
					xmy = p_in.x - p_in.y
				in
					vec2((errFunc(xpy) + errFunc(xmy))/2, (errFunc(xpy) - errFunc(xmy))/2) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
