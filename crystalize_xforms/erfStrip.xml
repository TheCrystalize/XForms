<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="erfStrip">
		<node name="input_params">
			<real name="erfStrip">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				erfStrip_init_result(erfStrip)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				vec2(errFunc(p_in.x), p_in.y) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
