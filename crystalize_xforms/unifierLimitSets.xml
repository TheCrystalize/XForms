<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="unifierLimitSets">
		<node name="input_params">
			<real name="unifierLimitSets">1</real>
			<real name="unifierLimitSets_D_re">1</real>
			<real name="unifierLimitSets_D_im">1</real>
			<real name="unifierLimitSets_mode" minval="1" maxval="2">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="dre" />
			<real name="dim" />
			<real name="mode" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				unifierLimitSets_init_result(unifierLimitSets, unifierLimitSets_D_re, unifierLimitSets_D_im, unifierLimitSets_mode)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					d = complex(dre, dim)
					left = 1 / (-z + d)
					right = (d * z - 1) / z
					m1 = if(p_in.x < 0, left, z)
					m2 = if(p_in.x < 0, left, if(p_in.x > dre, right, z))
				in
					toVec2(if(mode < 1.5, m1, m2)) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
