<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="weierstrassP">
		<node name="input_params">
			<real name="weierstrassP">1</real>
			<real name="weierstrassP_a"minval="0.0"maxval="1.0">0.5</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="a" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				weierstrassP_init_result(weierstrassP, weierstrassP_a)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					r = weierstrassPfunc(p_in.x, a)
				in
					vec2(cos(p_in.y),sin(p_in.y)) * r * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
