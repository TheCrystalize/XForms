<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="weirdKleinianFinder">
		<node name="input_params">
			<real name="weirdKleinianFinder">1</real>
			<real name="shift">0</real>
			<real name="translate">0</real>
			<real name="scalar">1</real>
		</node>

		<node name="internal_params">
			<real name="scale" />
			<real name="shift" />
			<real name="translate" />
			<real name="scalar" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				weirdKleinianFinder_init_result(weirdKleinianFinder, shift, translate, scalar)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					imag1 = complex(0,1)
					imags = complex(0,shift)
					z0 = complex(p_in)
					z1 = (z0 + imag1) / (imag1 * z0 + 1.0)
					f = (((z1 + imags) / (conj(imags) * z1 + 1.0)) + complex(0, translate)) * scalar
				in
					toVec2(f) * scale
			]]>
		</string>
	</transform_def>


</user_transforms>
