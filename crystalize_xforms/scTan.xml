<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="scTan">
		<node name="input_params">
			<real name="scTan">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				scTan_init_result(scTan)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(tan(splitcomplex(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
