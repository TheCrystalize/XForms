<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="mathplaydough3">
		<node name="input_params">
			<real name="mathplaydough3">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				mathplaydough3_init_result(mathplaydough3)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				mat2x2(cos(length(p_in)), -sin(length(p_in)),
								sin(length(p_in)), sin(length(p_in))) * p_in * fmod(weight, length(p_in))
			]]>
		</string>
	</transform_def>
</user_transforms>
