<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="hypershiftComplex">
		<node name="input_params">
			<real name="hypershiftComplex">1</real>
			<real name="hypershiftComplex_real">0</real>
			<real name="hypershiftComplex_imag">0</real>
		</node>

		<node name="internal_params">
			<real name="scale" />
			<real name="real" />
			<real name="imag" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				hypershiftComplex_init_result(hypershiftComplex, hypershiftComplex_real, hypershiftComplex_imag)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					p = complex(real, imag)
					hs = (z + p) / (conj(p) * z + 1.0)
				in
					toVec2(hs) * scale
			]]>
		</string>
	</transform_def>


</user_transforms>
