<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="disc2Ellipse">
		<node name="input_params">
			<real name="disc2Ellipse">1</real>
			<real name="disc2EllipseA">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="A" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				disc2Ellipse_init_result(disc2Ellipse, disc2EllipseA)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in.x, p_in.y) * A
					f = (z + sqrt(z * z + 1.0))
					g = (f - 1.0) / (f + 1.0)
				in
					toVec2(g) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
