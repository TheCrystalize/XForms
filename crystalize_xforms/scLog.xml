<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="scLog">
		<node name="input_params">
			<real name="scLog">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				scLog_init_result(scLog)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(log(splitcomplex(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
