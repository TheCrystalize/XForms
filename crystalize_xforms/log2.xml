<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="log2">
		<node name="input_params">
			<real name="log2">1</real>
			<real name="log2_base">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="base" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				log2_init_result(log2, log2_base)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					c = complex(p_in)
					z = exp(r() * pi() * c)
					l = (z - 1.0) / (-z - 1.0)
				in
					toVec2(l) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
