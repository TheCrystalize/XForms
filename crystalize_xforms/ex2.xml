<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="ex2">
		<node name="input_params">
			<real name="ex2">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				ex2_init_result(ex2)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					c = sqrt(z * z + 1)/ctanh(z)
				in
					toVec2(c) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
