<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="blurStrip">
		<node name="input_params">
			<real name="blurStrip">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				blurStrip_init_result(blurStrip)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
					vec2(unit_rnd_0 - 0.5, unit_rnd_1 - 0.5 - p_in.y) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
