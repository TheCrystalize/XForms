<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="pbkDist">
		<node name="input_params">
			<real name="pbkDist">1</real>
			<real name="pbkDist_travel">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="t" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				pbkDist_init_result(pbkDist, pbkDist_travel)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					c0 = exp(log((z - 1.0) / (-z - 1.0)) * t)
					c = (-c0 + 1.0) / (c0 + 1.0)
				in
					toVec2(c) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
