<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="scSin">
		<node name="input_params">
			<real name="scSin">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				scSin_init_result(scSin)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(sin(splitcomplex(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
