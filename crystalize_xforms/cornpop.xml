<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="cornpop">
		<node name="input_params">
			<real name="cornpop">1</real>
			<real name="cornpop_r">1</real>
			<real name="cornpop_i">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="r" />
			<real name="i" />
		</node>

		<int name="num_unit_randoms">1</int>

		<string name="winter_init_function">
			<![CDATA[
				cornpop_init_result(cornpop, cornpop_r, cornpop_i)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					c = complex(p_in.x, p_in.y)
					p = c - r * ctan(c + ccot(i * c))
				in
					toVec2(p) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
