<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="sinusoidal2">
		<node name="input_params">
			<real name="sinusoidal2">1</real>
			<real name="sinusoidal2_x">1</real>
			<real name="sinusoidal2_y">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="x" />
			<real name="y" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				sinusoidal2_init_result(sinusoidal2, sinusoidal2_x, sinusoidal2_y)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					sx = p_in.x * pi() * 0.25 / x
					sy = p_in.y * pi() * 0.25 / y
				in
					vec2(sin(sx)*x,sin(sy)*y) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
