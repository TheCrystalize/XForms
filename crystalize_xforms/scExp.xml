<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="scExp">
		<node name="input_params">
			<real name="scExp">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				scExp_init_result(scExp)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(exp(splitcomplex(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
