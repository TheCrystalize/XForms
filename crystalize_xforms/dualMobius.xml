<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="dualMobius">
		<node name="input_params">
			<real name="dualMobius">1</real>
			<real name="ar">1</real>
			<real name="ai">0</real>
			<real name="br">0</real>
			<real name="bi">0</real>
			<real name="cr">0</real>
			<real name="ci">0</real>
			<real name="dr">1</real>
			<real name="di">0</real>
		</node>

		<node name="internal_params">
			<real name="scale" />
			<real name="ar" />
			<real name="ai" />
			<real name="br" />
			<real name="bi" />
			<real name="cr" />
			<real name="ci" />
			<real name="dr" />
			<real name="di" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				dualMobius_init_result(dualMobius, ar, ai, br, bi, cr, ci, dr, di)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					a = dual(ar, ai)
					b = dual(br, bi)
					c = dual(cr, ci)
					d = dual(dr, di)
					z = dual(p_in)
					mob = (a * z + b) / (c * z + d)
				in
					toVec2(mob) * scale
			]]>
		</string>
	</transform_def>


</user_transforms>
