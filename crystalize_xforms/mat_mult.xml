<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="mat_mult_d">
		<node name="input_params">
			<real name="mat_mult_d">1</real>
			<real name="mat_mult_d_iterations"minval="1"maxval="50">1</real>
			<real name="mat_mult_d_real">1</real>
			<real name="mat_mult_d_imag">0</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="iters" />
			<real name="mreal" />
			<real name="mimag" />
		</node>

		<int name="num_unit_randoms">1</int>

		<string name="winter_init_function">
			<![CDATA[
				mat_mult_d_init_result(mat_mult_d, mat_mult_d_iterations, mat_mult_d_real, mat_mult_d_imag)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					v = mat3x3(p_in.x, p_in.y, 0.0,
											mreal, mimag, 0.0,
											iters, 0.0, 0.0)
					m0 = mmit(v, 0)
					m = iterate(mmit, v)
				in
					vec2(m.e0, m.e1) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
