<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="newMaths">
		<node name="input_params">
			<real name="newMaths">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				newMaths_init_result(newMaths)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					f = (1.0 / (z + 2.0)) - (1.0 / z * (2.0 * z + 1.0))
				in
					toVec2(f) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
