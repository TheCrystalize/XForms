<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="julia_grid">
		<!-- Made by Crystalize! :D With some help from Bezo :) -->
		<node name="input_params">
			<real name="julia_grid">1</real>
			<real name="julia_grid_iterations"minval="0.0"maxval="2.0">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="iters" />
		</node>

		<int name="num_unit_randoms">1</int>

		<string name="winter_init_function">
			<![CDATA[
				julia_grid_init_result(julia_grid, julia_grid_iterations * 10.0)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					v = vec4(p_in.x, p_in.y, unit_rnd_0, iters)
					v0 = jgrid(v, 0)
					nv = iterate(jgrid, v)
				in
					vec2(nv.x, nv.y) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
