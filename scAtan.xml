<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="scAtan">
		<node name="input_params">
			<real name="scAtan">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				scAtan_init_result(scAtan)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(atan(splitcomplex(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
