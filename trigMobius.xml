<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="trigMobius">
		<node name="input_params">
			<real name="trigMobius">1</real>
			<real name="trigMobius_a">1</real>
			<real name="trigMobius_b">1</real>
			<real name="trigMobius_c">1</real>
			<real name="trigMobius_d">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
			<real name="a" />
			<real name="b" />
			<real name="c" />
			<real name="d" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				trigMobius_init_result(trigMobius, trigMobius_a, trigMobius_b, trigMobius_c, trigMobius_d)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in)
					f = csinh(a * z + b) / ccosh(c * z + d)
				in
					toVec2(f) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
