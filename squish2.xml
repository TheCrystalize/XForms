<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="squish2">
		<node name="input_params">
			<real name="squish2">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				squish2_init_result(squish2)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					r = 1 - (1 / length(p_in))
					a = atan2(p_in.y, p_in.x)
				in
					vec2(cos(a), sin(a)) * r * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
