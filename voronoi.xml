<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="voronoi">
		<node name="input_params">
			<real name="voronoi">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">1</int>

		<string name="winter_init_function">
			<![CDATA[
				voronoi_init_result(voronoi)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
				in
					p_in * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
