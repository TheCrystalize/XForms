<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="collatz">
		<node name="input_params">
			<real name="collatz">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<int name="num_unit_randoms">2</int>

		<string name="winter_init_function">
			<![CDATA[
				collatz_init_result(collatz)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					z = complex(p_in.x, p_in.y)
					Cz = (7*z+2.0-ccos(pi()*z)*(5*z+2.0))/4.0
				in
					toVec2(Cz) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
