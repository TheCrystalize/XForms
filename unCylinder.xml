<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="unCylinder">
		<node name="input_params">
			<real name="unCylinder">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>


		<string name="winter_init_function">
			<![CDATA[
				unCylinder_init_result(unCylinder)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
					vec2(square((p_in.x - 1.0) / p_in.x) * p_in.x + 2.0, -p_in.y) * 0.5 * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
