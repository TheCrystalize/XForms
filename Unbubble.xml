<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="unBubble">
		<node name="input_params">
			<real name="unBubble">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				unBubble_init_result(unBubble)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				let
					r = dot(p_in, p_in)
				in
					(p_in * 2.0 * (sqrt2() - sqrt(2.0 - r)) / r) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
