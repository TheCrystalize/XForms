<?xml version="1.0" encoding="utf-8"?>
<user_transforms>
	<transform_def name="dualLog">
		<node name="input_params">
			<real name="dualLog">1</real>
		</node>

		<node name="internal_params">
			<real name="weight" />
		</node>

		<string name="winter_init_function">
			<![CDATA[
				dualLog_init_result(dualLog)
			]]>
		</string>

		<string name="winter_eval_function">
			<![CDATA[
				toVec2(log(dual(p_in))) * weight
			]]>
		</string>
	</transform_def>


</user_transforms>
